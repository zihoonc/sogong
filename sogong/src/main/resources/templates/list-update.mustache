<body class="sub_page">

<div class="hero_area">
    <div class="bg-box">
        <img src="assets/images/hero-bg.jpg" alt="">
    </div>
    <!-- header section strats -->
    {{>layout/header}}
    <!-- end header section -->
</div>

<!-- book section -->
<section class="book_section layout_padding">

    <div class="container">
        <div class="heading_container">
            <h2>
                Book A Table
            </h2>
        </div>
        <div class="row">
            <div class="col-md-4">


                {{#book}}
                    <form action="/edit/{{bookingId}}" method="POST">
                        <input type="hidden" name="_method" value="PUT"/>
                        <div class="form-group">
                            <label for="bookingId">번호</label>
                            <input type="text" class="form-control" id="bookingId" name="bookingId" value="{{bookingId}}"readonly>
                        </div>
                        <div class="form-group">
                            <label for="name">이름</label>
                            <input type="text" class="form-control" id="name" name="name" value="{{name}}"readonly>
                        </div>
                        <div class="form-group">
                            <label for="email">이메일 </label>
                            <input type="text" class="form-control" id="email" name="email" value="{{email}}" readonly>
                        </div>
                        <div class="form-group">
                            <label for="phone">전화번호 </label>
                            <input type="text" class="form-control" id="phone" name="phone" value="{{phone}}"readonly >
                        </div>
                        <div class="form-group">
                            <label for="people">사람 수</label>
                            <input type="text" class="form-control" id="people" name="people" value="{{people}}"readonly>
                        </div>
                        <div class="form-group">
                            <label for="bookDay">사람 수</label>
                            <input type="text" class="form-control" id="bookDay" name="bookDay" value="{{bookDay}}" readonly>
                        </div>
                        <div class="form-group">
                            <label for="time">time </label>
                            <input type="text" class="form-control" id="time" name="time" value="{{time}}"readonly>
                        </div>
                        <div class="form-group">
                            <label for="tableNum">tableNum</label>
                            <input type="text" class="form-control" id="tableNum" name="tableNum" value="{{tableNum}}"readonly>
                        </div>
                        <div class="form-group">
                            <label for="arrive">도착시간</label>
                            <input type="time" class="form-control" id="arrive" name="arrive" >
                        </div>
                        <div class="btn_box">
                            <button type="submit">
                                도착 시간 입력
                            </button>
                        </div>
                    </form>
                {{/book}}

            </div>


        </div>
    </div>
</section>
<!-- end book section -->

<!-- footer section -->
{{>layout/footer}}
<!-- footer section -->

<!-- jQery -->
<script src="assets/js/jquery-3.4.1.min.js"></script>
<!-- popper js -->
