    <link href="assets/css/font-awesome.min.css" rel="stylesheet" />

    <!-- Custom styles for this template -->
    <link href="assets/css/style.css" rel="stylesheet" />
    <!-- responsive style -->
    <link href="assets/css/responsive.css" rel="stylesheet" />
    <!-- loadHTML -->
    <script src="assets/js/includeHTML.js"></script>
    <!-- end loadHTML -->
    <!-- timepicker -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.js"></script>

</head>

<body class="sub_page">

<div class="hero_area">
    <div class="bg-box">
        <img src="assets/images/hero-bg.jpg" alt="">
    </div>
    <!-- header section strats -->
    {{>layout/header}}
    <!-- end header section -->
</div>

<!-- book section -->
<section class="book_section layout_padding">
    <div class="container">
        <div class="heading_container">
            <h2>
                Book Modify
            </h2>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="form_container">
                    <form action="/bookModify" method="POST" onSubmit="return checkForm()" >
                        {{#book}}
                        <div>
<!--                            <input type="text" class="form-control" name="name" value="{{bookingId}}"/>-->
                            <input type="text" class="form-control" name="name" value="{{name}}"/>
                        </div>
                        <div>
                            <input type="text" class="form-control" name="phone" value="{{phone}}" />
                        </div>
                        <div>
                            <input type="email" class="form-control" name="email" value="{{email}}" />
                        </div>
                        <div>
                            <select name="people" id="people" class="form-control nice-select wide">
                                <option value="{{people}}" selected>
                                    {{people}}
                                </option>
                                <option value="2">
                                    2
                                </option>
                                <option value="3">
                                    3
                                </option>
                                <option value="4">
                                    4
                                </option>
                                <option value="5">
                                    5
                                </option>
                            </select>
                        </div>
                        <div>
                            <input type="date" id="date" name="bookDay" class="form-control" value="{{bookDay}}">
                        </div>
                        <input type="text" class="form-control" name="time" id="time" value="{{time}}" readonly />
                        <input type="text" class="form-control" name="tableNum" id="tableNum" value="{{tableNum}}" readonly />
                        <div class="btn_box">
                            <button type="submit">
                                Book Modify
                            </button>
                        </div>
                        {{/book}}
                    </form>

                    <form action="/bookDelete" method="POST" onSubmit="return checkForm()" method="POST">
                        <button type="submit">
                            Book Delete
                        </button>
                    </form>
                </div>
            </div>

            <div class="col-md-6">
                <div class="col text-center" style=" float: none; margin: 0 auto; margin-left: 50px; margin-bottom: 50px;">
                    <!--<input type="date" class="form-control" style="align-items: center; margin-right: 15px;">-->
                    <input type="text" id="timepicker" class="timepicker"
                           style="align-items: center; text-align: center;">
                    <button class="btn btn-warning" type="button" onclick="select()">select</button>
                </div>
                <div class="row" style="width: 30%; margin: 0 auto;">

                    <div class="col d-flex" style="margin-bottom: 30px;">
                        <div class="col d-flex justify-content-start align-items-start">
                            <button class="btn btn-primary" type="button" data-toggle="btn btn-disabled" onclick="selected(this);">
                                1
                            </button>
                        </div>
                        <div class="col  d-flex justify-content-start align-items-center">
                            <button class="btn btn-primary" type="button" onclick="selected(this);">
                                2
                            </button>
                        </div>
                        <div class="col d-flex justify-content-start align-items-end">
                            <button class="btn btn-primary" type="button" onclick="selected(this);">
                                3
                            </button>
                        </div>
                    </div>

                    <div class="col d-flex" style="margin-bottom: 30px;">
                        <div class="col d-flex justify-content-center align-items-start">
                            <button class="btn btn-primary" type="button" onclick="selected(this);">
                                4
                            </button>
                        </div>
                        <div class="col d-flex justify-content-center align-items-center">
                            <button class="btn btn-primary" type="button" onclick="selected(this);">
                                5
                            </button>
                        </div>
                        <div class="col d-flex justify-content-center align-items-end">
                            <button class="btn btn-primary" type="button" onclick="selected(this);">
                                6
                            </button>
                        </div>
                    </div>

                    <div class="col d-flex">
                        <div class="col  d-flex justify-content-end align-items-start">
                            <button class="btn btn-primary" type="button" onclick="selected(this);">
                                7
                            </button>
                        </div>
                        <div class="col  d-flex justify-content-end align-items-center">
                            <button class="btn btn-primary" type="button" onclick="selected(this);">
                                8
                            </button>
                        </div>
                        <div class="col d-flex justify-content-end align-items-end">
                            <button class="btn btn-primary" type="button" onclick="selected(this);">
                                9
                            </button>
                        </div>
                    </div>
                </div>
                </br>
                <div style="margin-left: 210px; margin-top: 30px;">
                    <span> Selected <a id="tableNumber"> {{#book}}{{tableNum}}{{/book}} </a> numbered seat. </span>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- end book section -->

<!-- footer section -->
{{>layout/footer}}
<!-- footer section -->

<!-- jQery -->
<script src="assets/js/jquery-3.4.1.min.js"></script>
<!-- popper js -->
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous">
</script>
<!-- bootstrap js -->
<script src="assets/js/bootstrap.js"></script>
