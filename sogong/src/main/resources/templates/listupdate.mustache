<body class="sub_page">

<div class="hero_area">
    <div class="bg-box">
        <img src="assets/images/hero-bg.jpg" alt="">
    </div>
    <!-- header section strats -->
    {{>layout/header}}
    <!-- end header section -->
</div>

<!-- book section -->
<section class="book_section layout_padding">
    <div class="container">
        <div class="heading_container">
            <h2>
                Book A Table
            </h2>
        </div>
        <div class="row">
            <div class="col-md-4">
                <form action="/books/update/{{bookingId}}" method="post">
                    <input type="hidden" name="_method" value="put"/>
                    <div class="form-group">
                        <label for="id">번호 </label>
                        <input type="text" class="form-control" id="id" value="{{book.bookingId}}" readonly>
                    </div>
                    <div class="form-group">
                        <label for="title">이름</label>
                        <input type="text" class="form-control" id="name" value="{{book.name}} "readonly>
                    </div>

                    <div class="form-group">
                        <label for="author">이메일 </label>
                        <input type="text" class="form-control" id="email" value="{{book.email}}" readonly>
                    </div>
                    <div class="form-group">
                        <label for="author">사람 수 </label>
                        <input type="text" class="form-control" id="people" value="{{book.people}}" readonly>
                    </div>
                    <div class="form-group">
                        <label for="author">예약 날짜 </label>
                        <input type="text" class="form-control" id="bookDay" value="{{book.bookDay}}" readonly>
                    </div>
                    <div class="form-group">
                        <label for="author">time </label>
                        <input type="text" class="form-control" id="time" value="{{book.time}}" readonly>
                    </div>
                    <div class="form-group">
                        <label for="author">tableNum</label>
                        <input type="text" class="form-control" id="tableNum" value="{{book.tableNum}}" readonly >
                    </div>
                    <div class="form-group">
                        <label for="content">도착시간</label>
                        <input type="text" class="form-control" id="arrive" Placeholder="도착 시간">{{^book.arrrive}}{{/book.arrrive}}</input>
                    </div>
                    <a href="/list" role="button" class="btn btn-secondary">취소</a>
                    <div class="btn_box">
                        <button type="submit">
                            도착완료
                        </button>
                    </div>
                </form>

            </div>
    </div>
</section>
<!-- end book section -->

<!-- footer section -->
{{>layout/footer}}
<!-- footer section -->

<!-- jQery -->
<script src="assets/js/jquery-3.4.1.min.js"></script>
